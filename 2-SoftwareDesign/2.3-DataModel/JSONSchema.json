{
  "type": "object",
  "title": "Record",
  "description": "Record of users, Consumers, annoyance, tempreture and notifications",
  "properties": {
    "Users": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "maxLength": 30
        },
        "email": {
          "type": "string",
          "maxLength": 30
        },
        "age": {
          "type": "integer",
          "minimum": 0,
          "maximum": 100
        },
        "role": {
          "type": "string",
          "maxLength": 30
        }
      },
      "required": ["name", "email", "age", "role"]
    },
    "Consumers": {
      "type": "object",
      "properties": {
        "previous_annoyance_level": {
          "type": "object",
          "propertires": {
            "check_time": {
              "type": "string",
              "format": "date-time"
            },
            "annoyance_level": {
              "type": "string",
              "maxLength": 20
            }
          },
          "required": ["check_time", "annoyance_level"]
        },
        "previous_tempreture_level": {
          "type": "object",
          "propertires": {
            "check_time": {
              "type": "string",
              "format": "date-time"
            },
            "actual_tempreture": {
              "type": "number",
              "maximum": 60
            },
            "feeling_tempreture": {
              "type": "number",
              "maximum": 60
            },
            "humidity": {
              "type": "number",
              "maximum": 100,
              "minimum": 0
            },
            "wind_speed": {
              "type": "number",
              "maximum": 400,
              "minimum": 0
            }
          },
          "required": [
            "check_time",
            "actual_tempreture",
            "feeling_tempreture",
            "humidity",
            "wind_speed"
          ]
        }
      },
      "required": []
    },
    "Technical_Support": {
      "type": "object",
      "properties": {
        "specialization": {
          "type": "string",
          "maxLength": 300
        },
        "position": {
          "type": "string",
          "maxLength": 30
        }
      },
      "required": ["specialization", "position"]
    },
    "World_Weather_Organization": {
      "type": "object",
      "properties": {
        "actual_tempreture": {
          "type": "number",
          "maximum": 60,
          "miniimmum": -90
        },
        "feeling_tempreture": {
          "type": "number",
          "maximum": 60,
          "miniimmum": -90
        },
        "humidity": {
          "type": "number",
          "maximum": 100,
          "minimum": 0
        },
        "wind_speed": {
          "type": "number",
          "maximum": 400,
          "minimum": 0
        }
      },
      "required": [
        "actual_tempreture",
        "feeling_tempreture",
        "humidity",
        "wind_speed"
      ]
    },
    "annoyance": {
      "type": "object",
      "properties": {
        "check_time": {
          "type": "string",
          "format": "date-time"
        },
        "annoyance_level": {
          "type": "string",
          "maxLength": 20
        }
      },
      "required": ["check_time", "annoyance_level"]
    },
    "tempreture": {
      "type": "object",
      "properties": {
        "check_time": {
          "type": "string",
          "format": "date-time"
        },
        "actual_tempreture": {
          "type": "number",
          "maximum": 60,
          "minimum": -90
        },
        "feeling_tempreture": {
          "type": "number",
          "maximum": 60,
          "minimum": -90
        },
        "humidity": {
          "type": "number",
          "maximum": 100,
          "minimum": 0
        },
        "wind_speed": {
          "type": "number",
          "maximum": 400,
          "minimum": 0
        }
      },
      "required": [
        "check_time",
        "actual_tempreture",
        "feeling_tempreture",
        "humidity",
        "wind_speed"
      ]
    },
    "notification": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "maxLength": 30
        },
        "topic": {
          "type": "string",
          "maxLength": 30
        },
        "text": {
          "type": "number",
          "maxLength": 200
        }
      },
      "required": ["type", "topic", "text"]
    }
  },
  "required": ["Users"]
}
